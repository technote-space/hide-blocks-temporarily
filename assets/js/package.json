{
  "name": "hide-blocks-temporarily",
  "version": "2.1.6",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@technote-space/gutenberg-test-helper": "^0.1.17",
    "@technote-space/gutenberg-utils": "^2.4.3",
    "@wordpress/block-editor": "^6.1.9",
    "@wordpress/blocks": "^9.1.5",
    "@wordpress/components": "^14.1.6",
    "@wordpress/compose": "^4.1.3",
    "@wordpress/data": "^5.1.3",
    "@wordpress/edit-post": "^4.1.15",
    "@wordpress/element": "^3.1.1",
    "@wordpress/hooks": "^3.1.1",
    "@wordpress/plugins": "^3.1.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "speed-measure-webpack-plugin": "^1.5.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  }
}
