{
  "name": "hide-blocks-temporarily",
  "version": "0.0.1",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "webpack --mode development --config index.config.js",
    "build": "webpack --mode production --config index.config.js"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "ie": 11
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "webpack": "^4.30.0",
    "webpack-cli": "~3.1.2"
  }
}
