{
  "name": "hide-blocks-temporarily",
  "version": "2.0.9",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "update": "ncu -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@technote-space/gutenberg-test-helper": "^0.0.19",
    "@technote-space/gutenberg-utils": "^2.2.10",
    "@wordpress/block-editor": "^4.3.7",
    "@wordpress/blocks": "^6.20.3",
    "@wordpress/components": "^10.0.6",
    "@wordpress/compose": "^3.19.3",
    "@wordpress/data": "^4.22.3",
    "@wordpress/edit-post": "^3.21.7",
    "@wordpress/element": "^2.16.0",
    "@wordpress/hooks": "^2.9.0",
    "@wordpress/plugins": "^2.20.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "react": "^16.13.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "webpack": "^4.44.1",
    "webpack-cli": "~3.3.12"
  }
}
