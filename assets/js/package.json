    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@wordpress/block-library": "^2.4.6",
    "@wordpress/blocks": "^6.3.0",
    "@wordpress/data": "^4.5.0",
    "@wordpress/editor": "^9.2.6",
    "babel-loader": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
