{
  "name": "hide-blocks-temporarily",
  "version": "2.1.6",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.10",
    "@technote-space/gutenberg-test-helper": "^0.1.13",
    "@technote-space/gutenberg-utils": "^2.4.3",
    "@wordpress/block-editor": "^5.3.0",
    "@wordpress/blocks": "^8.0.0",
    "@wordpress/components": "^13.0.0",
    "@wordpress/compose": "^3.25.0",
    "@wordpress/data": "^4.27.0",
    "@wordpress/edit-post": "^3.27.0",
    "@wordpress/element": "^2.20.0",
    "@wordpress/hooks": "^2.12.0",
    "@wordpress/plugins": "^2.25.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "speed-measure-webpack-plugin": "^1.4.2",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0"
  }
}
