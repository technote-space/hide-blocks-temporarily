{
  "name": "hide-blocks-temporarily",
  "version": "1.2.0",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint src/**/**/*.js && eslint __tests__/**/**/*.js",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "GPL-3.0",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@technote-space/gutenberg-utils": "^1.2.6",
    "babel-loader": "^8.0.6",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "webpack": "^4.40.2",
    "webpack-cli": "~3.3.8"
  },
  "devDependencies": {
    "@wordpress/block-editor": "^3.1.0",
    "@wordpress/block-library": "^2.8.0",
    "@wordpress/blocks": "^6.6.0",
    "@wordpress/components": "^8.2.0",
    "@wordpress/compose": "^3.6.0",
    "@wordpress/core-data": "^2.6.0",
    "@wordpress/data": "^4.8.0",
    "@wordpress/editor": "^9.6.0",
    "@wordpress/element": "^2.7.0",
    "@wordpress/format-library": "^1.8.0",
    "@wordpress/hooks": "^2.6.0",
    "@wordpress/i18n": "^3.6.0",
    "@wordpress/rich-text": "^3.6.0",
    "@wordpress/server-side-render": "^1.2.0",
    "@wordpress/url": "^2.7.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
