{
  "name": "hide-blocks-temporarily",
  "version": "2.0.10",
  "description": "This plugin makes it easy to hides blocks temporarily.",
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit"
  },
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@technote-space/gutenberg-test-helper": "^0.0.24",
    "@technote-space/gutenberg-utils": "^2.2.14",
    "@wordpress/block-editor": "^5.1.0",
    "@wordpress/blocks": "^6.24.0",
    "@wordpress/components": "^11.1.0",
    "@wordpress/compose": "^3.22.0",
    "@wordpress/data": "^4.25.0",
    "@wordpress/edit-post": "^3.25.0",
    "@wordpress/element": "^2.18.0",
    "@wordpress/hooks": "^2.10.0",
    "@wordpress/plugins": "^2.23.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.12.0",
    "eslint-plugin-react": "^7.21.5",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^26.6.1",
    "jest-extended": "^0.11.5",
    "react": "^17.0.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "webpack": "^5.2.0",
    "webpack-cli": "~4.1.0"
  }
}
